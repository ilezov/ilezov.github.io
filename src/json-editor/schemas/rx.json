[  
  {
    "regex": [
      "^.*(\\[\"|\\.)binding.*"
    ],
    "type": [
      "object"
    ],
    "readOnly": true,
    "required": "inherit",
    "hidden": true,
    "inactive": true,
    "description": "Data Model field mapping to Data Source"
  },
  {
    "regex": [
      "^.*(\\[\"|\\.)staticValue.*"
    ],
    "type": [
      "object",
      "string"
    ],
    "hidden": true
  },
  {
    "regex": [
      "^vars\\..*",
      "^vars\\[.*"
    ],
    "description": "Shared variables can be defined as data values that can be shared by one or more visual control options.<br>View and Control Options may be linked to shared variables and updated dynamically when a variable changes.<br>Data Model elements may also be used to populate the variables and trigger global changes as result of query execution on the back-end."
  },
  {
    "regex": [
      "^graphic\\.elements\\[\"group\"\\]\\.children.*"
    ],
    "pathMod": {
      "replace": "\\[\"group\"\\]\\.children",
      "to": ""
    }
  },
  {
    "regex": [
      "^series\\[\"tree\"\\]\\.data\\[\"object\"\\]\\.children.*"
    ],
    "pathMod": {
      "replace": "\\[\"object\"\\]\\.children",
      "to": ""
    }
  },
  {
    "regex": [
      "^series\\[\"treemap\"\\]\\.data\\[\"object\"\\]\\.children.*"
    ],
    "pathMod": {
      "replace": "\\[\"object\"\\]\\.children",
      "to": ""
    }
  },
  {
    "regex": [
      "^controls\\[.*\"splitPane\"\\]\\.controls$"
    ],
    "pathMod": {
      "replace": "^controls\\[.*\\.controls",
      "to": "controls"
    },
    "maxLength":3,
    "minLength":3,
    "readOnly": true,
    "required": true
  },  
  {
    "regex": [
      "^controls\\[.*\"splitPane\"\\]\\.controls\\[\"layoutContainer\"\\]$"
    ],
    "pathMod": {
      "replace": "^controls\\[.*\\.controls",
      "to": "controls"
    },
    "title":"pane",    
    "required": true    
  },
  {
    "regex": [
      "^controls\\[.*\"splitPane\"\\]\\.controls\\[\"splitter\"\\]$"
    ],
    "pathMod": {
      "replace": "^controls\\[.*\\.controls",
      "to": "controls"
    },
    "hidden":false    
  },
  {
    "regex": [
      "^controls\\[.*\"splitPane\"\\]\\.controls\\[\"layoutContainer\"\\]\\.style$"
    ],
    "pathMod": {
      "replace": "^controls\\[.*\\.controls",
      "to": "controls"
    },
    "default":{}
  },
  {
    "regex": [
      "^controls\\[.*\"splitPane\"\\]\\.controls\\[\"layoutContainer\"\\]\\.style\\.normal$"
    ],
    "pathMod": {
      "replace": "^controls\\[.*\\.controls",
      "to": "controls"
    },
    "default":{
      "top":0,"right":0,"bottom":0,"left":0, "width":"auto","height":"auto"
    }
  },
  {
    "regex": [
      "^controls\\[.*\"splitPane\"\\]\\.controls\\[\"layoutContainer\"\\]\\.style\\.normal\\.top$",
      "^controls\\[.*\"splitPane\"\\]\\.controls\\[\"layoutContainer\"\\]\\.style\\.normal\\.right$",
      "^controls\\[.*\"splitPane\"\\]\\.controls\\[\"layoutContainer\"\\]\\.style\\.normal\\.bottom$",
      "^controls\\[.*\"splitPane\"\\]\\.controls\\[\"layoutContainer\"\\]\\.style\\.normal\\.left$",
      "^controls\\[.*\"splitPane\"\\]\\.controls\\[\"layoutContainer\"\\]\\.style\\.normal\\.width$",
      "^controls\\[.*\"splitPane\"\\]\\.controls\\[\"layoutContainer\"\\]\\.style\\.normal\\.height$"
    ],
    "pathMod": {
      "replace": "^controls\\[.*\\.controls",
      "to": "controls"
    },
    "hidden":true
  },
  {
    "regex": [
      "^controls\\[.*\\.controls.*"
    ],
    "pathMod": {
      "replace": "^controls\\[.*\\.controls",
      "to": "controls"
    }
  },
  {
    "regex": [
      "^visualMap\\[.*\\.inRange\\.[^\\[\\]]+"
    ],
    "type": [
      "array"
    ],
    "items": {
      "string": {
        "type": [
          "string"
        ]
      },
      "number": {
        "type": [
          "number"
        ]
      },
      "color": {
        "type": [
          "color"
        ]
      }
    }
  },
  {
    "regex": [
      "^visualMap\\[.*\\.inRange\\.+\\[\"number\"\\]"
    ],
    "type": [
      "number"
    ]
  },
  {
    "regex": [
      "^visualMap\\[.*\\.inRange\\.+\\[\"string\"\\]"
    ],
    "type": [
      "string"
    ]
  },
  {
    "regex": [
      "^visualMap\\[.*\\.inRange\\.+\\[\"color\"\\]"
    ],
    "type": [
      "color"
    ]
  },
  {
    "regex": [
      "controls\\[\"grid\"\\]\\.data\\[\"object\"\\]\\.cells\\[\".+\"\\]\\..+\\.value$"
    ],
    "description": "Value of this property will be submitted to back-end as chosen data source parameter.",
    "title":"parameter"
  },
  {
    "regex": [
      "controls\\[\"grid\"\\]\\.data\\[\"object\"\\]\\.cells\\[\".+\"\\]\\..+\\.value\\..+"
    ],
    "hidden": true
  }
]